{
    "name": "terminal42/contao-html-injection",
    "type": "contao-bundle",
    "description": "Inject custom HTML code into Contao page output.",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "http://terminal42.ch"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "contao/core-bundle": "^4.1",
        "symfony/http-kernel": "~2.7 || ~3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\HtmlInjectionBundle\\": "src"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\HtmlInjectionBundle\\ContaoManager\\Plugin"
    }
}
